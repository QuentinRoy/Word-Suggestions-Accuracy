{
  "name": "exii-accuracy-control",
  "version": "1.2.0",
  "private": true,
  "dependencies": {
    "@hcikit/tasks": "^0.3.0",
    "@hcikit/workflow": "^0.3.0",
    "@material-ui/core": "^4.3.1",
    "@material-ui/styles": "^4.3.0",
    "aws-sdk": "^2.513.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "d3-array": "^2.2.0",
    "d3-scale": "^3.0.1",
    "formik": "^1.5.8",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "loglevel": "^1.6.3",
    "papaparse": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-device-detect": "^1.7.5",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "react-simple-keyboard": "^1.26.5",
    "react-spring": "^8.0.27",
    "sanitize-filename": "^1.6.2",
    "short-uuid": "^3.1.1",
    "shortid": "^2.2.14",
    "styled-components": "^4.3.2",
    "xml2js": "^0.4.19"
  },
  "homepage": ".",
  "scripts": {
    "start": "cross-env REACT_APP_VERSION=$npm_package_version REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts start",
    "build": "cross-env REACT_APP_VERSION=$npm_package_version REACT_APP_GIT_SHA=`git rev-parse --short HEAD` GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate-sks-distribution": "node scripts/sks-distribution-generator/index.js",
    "deploy": "aws s3 sync build/ s3://exii-accuracy-control",
    "predeploy": "npm run build",
    "export-logs": "python3 scripts/loggings/export_runs.py && python3 scripts/loggings/export_trials.py && python3 scripts/loggings/export_events.py",
    "download-logs": "node scripts/download-logs.js",
    "associate-qualification": "node scripts/associate-participant-qualification.js",
    "log-workflow": "npm run download-logs && npm run associate-qualification && npm run export-logs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 3 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.2.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
