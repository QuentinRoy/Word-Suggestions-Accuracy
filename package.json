    "@hcikit/workflow": "^0.3.0",
    "@material-ui/core": "^4.8.0",
    "@material-ui/styles": "^4.7.1",
    "aws-sdk": "^2.590.0",
    "classnames": "^2.2.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.2",
    "d3-array": "^2.4.0",
    "d3-scale": "^3.2.1",
    "formik": "^2.0.8",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "loglevel": "^1.6.6",
    "papaparse": "^5.1.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-simple-keyboard": "^2.1.54",
    "react-spring": "^8.0.27",
    "sanitize-filename": "^1.6.3",
    "short-uuid": "^3.1.1",
    "shortid": "^2.2.15",
    "styled-components": "^4.4.1",
    "xml2js": "^0.4.22"
  },
  "homepage": ".",
  "scripts": {
    "start": "cross-env REACT_APP_VERSION=$npm_package_version REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts start",
    "build": "cross-env REACT_APP_VERSION=$npm_package_version REACT_APP_GIT_SHA=`git rev-parse --short HEAD` GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "test-scripts": "jest scripts",
    "eject": "react-scripts eject",
    "generate-sks-distribution": "node scripts/sks-distribution-generator/index.js",
    "generate-design": "node scripts/design/index.js",
    "deploy": "aws s3 sync build/ s3://exii-accuracy-control",
    "predeploy": "npm run build",
    "export-logs": "python3 scripts/loggings/export_runs.py && python3 scripts/loggings/export_trials.py && python3 scripts/loggings/export_events.py",
    "download-logs": "node scripts/download-logs.js",
    "associate-qualification": "node scripts/associate-participant-qualification.js",
    "log-workflow": "npm run download-logs && npm run associate-qualification && npm run export-logs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 3 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.3"
  }
}
